<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <!-- Plugin Informations -->
    <name>contextinclude</name>
    <class>fr.paris.lutece.plugins.contextinclude.service.ContextIncludePlugin</class>
    <version>1.0.0</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>contextinclude.plugin.description</description>
    <provider>contextinclude.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/contextinclude/contextinclude.png</icon-url>
    <copyright>Copyright 2001-2012 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>3.1.2</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>

    <!--Administration interface parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>CONTEXT_INCLUDE_MANAGEMENT</feature-id>
            <feature-title>contextinclude.adminFeature.contextinclude_management.name</feature-title>
            <feature-description>contextinclude.adminFeature.contextinclude_management.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/contextinclude/ManageContexts.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/contextinclude/contextinclude.png</feature-icon-url>
            <feature-documentation-url>jsp/admin/documentation/AdminDocumentation.jsp?doc=admin-contextinclude</feature-documentation-url>
        </admin-feature>
    </admin-features>
    
    <!-- Page Include Service -->
    <page-include-services>
        <page-include-service>
            <page-include-service-id>contextinclude</page-include-service-id>
            <page-include-service-class>fr.paris.lutece.plugins.contextinclude.web.ContextInclude</page-include-service-class>
        </page-include-service>
    </page-include-services>
</plug-in>